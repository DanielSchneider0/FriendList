<p style="color: Black"><%= notice %></p>

<div class="jumbotron">
  <h1 class="display-4"><%= @friend.first_name %> <%= @friend.last_name %></h1>
  <h3 class="my-4"><%= @friend.email%></h3><h3 class="my-4"><%= @friend.phone %></h3>
  <%= link_to "Edit this friend", edit_friend_path(@friend),  class:"btn btn-primary" %> 
